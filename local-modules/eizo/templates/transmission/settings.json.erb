{
<%- @config.each do |directive, value| -%>
    "<%= directive %>": <%= if value.respond_to?('join') then value.join(',') else %q{value} end %>
<%- end -%>

    "download-dir": "<%= @download %>/complete",
    "incomplete-dir": "<%= @download %>/incomplete",
    "incomplete-dir-enabled": true,
    "watch-dir": "<%= @download %>/watch",
    "watch-dir-enable": true
}
